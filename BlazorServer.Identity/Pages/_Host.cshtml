@page "/"
@namespace BlazorServer.Identity.Pages
@using BlazorServer.Identity.Models
@using Microsoft.AspNetCore.Mvc.TagHelpers
@inject Microsoft.AspNetCore.Antiforgery.IAntiforgery Xsrf
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "_Layout";
    var appInitialState = new InitialApplicationState()
    {
        AntiForgeryToken = Xsrf.GetAndStoreTokens(HttpContext).RequestToken
    };
}

<component type="typeof(App)" param-InitialState="@appInitialState" render-mode="ServerPrerendered" />
